<!DOCTYPE html>
<html class="h-100">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Consumo{% endblock %}</title>
        {% block stylesheets %}

            <link href="{{ asset('librerias/bootstrap4/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />


            <link href="{{ asset('librerias/fontawesome/web-font-with-css/css/all.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('librerias/DateTimePickerBootstrap4/build/css/bootstrap-datetimepicker.min.css') }}" rel="stylesheet" type="text/css" />

            <link href="{{ asset('librerias/datatables/DataTables-1.10.18/css/dataTables.bootstrap4.min.css') }}" rel="stylesheet" type="text/css" />

        {% endblock %}
    </head>
    <body class="d-flex flex-column h-100 bg-light">
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('librerias/jquery3/jquery.js') }}"></script>
            <script src="{{ asset('librerias/moment/moment-with-locales.js') }}"></script>
            <script src="{{ asset('librerias/DateTimePickerBootstrap4/build/js/bootstrap-datetimepicker.min.js') }}"></script>

            <script src="{{ asset('librerias/datatables/DataTables-1.10.18/js/jquery.dataTables.min.js') }}"></script>
            <script src="{{ asset('librerias/datatables/DataTables-1.10.18/js/dataTables.bootstrap4.min.js') }}"></script>

            <script src="{{ asset('librerias/CounterUp/jquery.waypoints.js') }}"></script>
            <script src="{{ asset('librerias/CounterUp/jquery.counterup.min.js') }}"></script>

            <script src="{{ asset('js/script.js') }}"></script>
        {% endblock %}
    </body>
</html>
